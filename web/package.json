{
  "name": "taskriot",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prod": "vite preview --port 3002",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "build:desktop": "cd ../service && bun install && rsync -a --delete --exclude node_modules ./ ../web/service-dist/ && cd ../web/service-dist && npm install --omit=dev && cd .. && cp $(which node) ./node && chmod +x ./node && npm install && vite build",
    "tauri:build": "npm run build:desktop && tauri build"
  },
