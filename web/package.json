    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@nivo/heatmap": "^0.99.0",
    "@tauri-apps/api": "2.5.0",
    "@tauri-apps/cli": "2.5.0",
    "dayjs": "^1.11.18",
    "i18next": "^25.6.1",
    "mobx": "^6.13.7",
    "mobx-react-lite": "^4.1.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
